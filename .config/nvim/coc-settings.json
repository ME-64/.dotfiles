{
  "suggest.noselect": false,
  "suggest.echodocSupport": true,
  "suggest.autoTrigger": "trigger",
  "suggest.enablePreselect": true,
  "suggest.preferCompleteThanJumpPlaceholder": false,
  "suggest.enablePreview": true,
  "suggest.floatEnable": true,
  "suggest.maxCompleteItemCount": 100,
  "suggest.localityBonus": false,
  "suggest.snippetIndicator": "~",
  "suggest.removeDuplicateItems": false,
  "suggest.lowPrioritySourceLimit": 3,
  "suggest.defaultSortMethod": "length",
  "suggest.detailMaxLength": 20,
  "suggest.labelMaxLength": 50,
  "suggest.completionItemKindLabels": {
    "keyword": "\uf1de",
    "variable": "\ue79b",
    "value": "\uf89f",
    "operator": "\u03a8",
    "function": "\u0192",
    "reference": "\ufa46",
    "constant": "\uf8fe",
    "method": "\uf09a",
    "struct": "\ufb44",
    "class": "\uf0e8",
    "interface": "\uf417",
    "text": "\ue612",
    "enum": "\uf435",
    "enumMember": "\uf02b",
    "module": "\uf40d",
    "color": "\ue22b",
    "property": "\ue624",
    "field": "\uf9be",
    "unit": "\uf475",
    "event": "\ufacd",
    "file": "\uf723",
    "folder": "\uf114",
    "snippet": "\ue60b",
    "typeParameter": "\uf728",
    "default": "\uf29c"
  },

  "diagnostic.refreshAfterSave": true,
  "diagnostic.refreshOnInsertMode": true,
  "diagnostic.errorSign": "",
  "diagnostic.warningSign": "",
  "diagnostic.messageDelay": 150,
  "diagnostic.enableHighlightLineNumber": false,
  "diagnostic.hintSign": "",
  "diagnostic.infoSign": "",
  "diagnostic.signOffset": 3000,
  "diagnostic.virtualText": false,

  "signature.enable": true,
  "signature.preferShownAbove": true,
  "signature.triggerSignatureWait": 50,
  "signature.target": "float",
  "signature.floatMaxWidth": 20,

  "coc.preferences.formatOnInsertLeave": false,
  "coc.preferences.bracketEnterImprove": true,
  "coc.preferences.colorSupport": false,
  "coc.preferences.formatOnSaveFiletypes": [
    "json",
    "html",
    "css",
    "javascript"
  ],

  "html.enable": true,
  "html.autoClosingTags": false,
  "html.filetypes": [
    "html",
    "handlebars",
    "htmldjango"
  ],
  "vimlsp.suggest.fromVimruntime": true,

  "css.colorDecorators.enable": false,

  "javascript.validate.enable": true,
  "javascript.suggestionActions.enabled": false,

  "tsserver.enable": false,

  "python.formatting.provider": "none",
  "python.jediEnabled": false,
  "python.linting.enabled": true,
  "python.linting.pylintEnabled": true,
  "python.autoComplete.addBrackets": true,
  "python.linting.pylintArgs": [
    "--disable=C0111",
    "--disable=C0103",
    "--disable=C0304",
    "--disable=invalid-name,missing-module-docstring,trailing-newlines"
  ],
  "python.linting.banditEnabled": false,

  "eslint.enable": true,
  "eslint.filetypes": [
      "javascript",
      "javascriptreact",
      "typescript",
      "typescriptreact"
  ]
}






